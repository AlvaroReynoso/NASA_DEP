{
	"name": "LS_API_key_vault",
	"properties": {
		"parameters": {
			"NASA_API_KEY": {
				"type": "string",
				"defaultValue": "@Microsoft.KeyVault(SecretName=’NASA-API-KEY’)"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "https://api.nasa.gov/insight_weather/?feedtype=json&ver=1.0&api_key=@{linkedService().NASA_API_KEY}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}